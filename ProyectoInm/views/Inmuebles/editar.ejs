<%- include ../layouts/cabecera.ejs %>  

<div class="card">
    <div class="card-header">
        Formulario de captura
    </div>
    <div class="card-body">
        <h5 class="card-title">Datos de inmueble Editar</h5>
        <p class="card-text">

            <form method="post" action="../actualizar/">
                <input type="text" name="id" value="<%=inmueble.idInmueble%>">
                <div class="form-group">
                    <label for="my-input">Nombre</label>
                    <input id="my-input" value="<%=inmueble.nombre%>" class="form-control" type="text" name="nombreInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">Dirección</label>
                    <input id="my-input" value="<%=inmueble.direccion%>" class="form-control" type="text" name="direccionInmueble">
                </div>
                
                <div class="form-group">
                    <label for="my-input">Zona</label>
                    <input id="my-input" value="<%=inmueble.zona%>" class="form-control" type="text" name="zonaInmueble">
                </div>
                
                <div class="form-group">
                    <label for="my-input">Precio</label>
                    <input id="my-input" value="<%=inmueble.precio%>" class="form-control" type="text" name="precioInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">estado</label>
                    <input id="my-input" value="<%=inmueble.estado%>" class="form-control" type="text" name="estadoInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">Municipio</label>
                    <input id="my-input" value="<%=inmueble.idMunicipio%>" class="form-control" type="text" name="municipioInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">Depósito</label>
                    <input id="my-input" value="<%=inmueble.deposito%>" class="form-control" type="text" name="depositoInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">NoDeposito</label>
                    <input id="my-input" value="<%=inmueble.noDepositos%>" class="form-control" type="text" name="noDepositoInmueble">
                </div>
            
                <div class="form-group">
                    <label for="my-input">User</label>
                    <input id="my-input" value="<%=inmueble.idUser%>" class="form-control" type="text" name="userInmueble">
                </div>
                <button class="btn btn-success" type="submit">Guardar</button>
                <a class="btn btn-primary" href="/anuncios">Cancelar</a>
            </form>

        </p>
    </div>
</div>


<%- include ../layouts/pie.ejs %>   